                android:layout_width="45dp"
                android:layout_height="43dp"
                android:layout_marginLeft="310dp"
                android:layout_marginTop="-110dp"
                app:srcCompat="@drawable/star" />

            <ImageView
                android:id="@+id/imageView333"
                android:layout_width="45dp"
                android:layout_height="43dp"
                android:layout_marginLeft="-10dp"
                android:layout_marginTop="-5dp"
                app:srcCompat="@drawable/lo" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="-40dp"
                android:drawableLeft="@android:drawable/ic_menu_mylocation"
                android:fontFamily="sans-serif-condensed-medium"
                android:lineSpacingExtra="34sp"
                android:text="Stanley Road,Prayagraj"
                android:textColor="#050505"
                android:textSize="18sp" />

            <TextView
                android:layout_width="269dp"
                android:layout_height="65dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="5dp"
                android:fontFamily="serif"
                android:lineSpacingExtra="10sp"
                android:text="Veg Plate - Rs.1200(per plate)     Non-Veg - Rs.1800(per plate)"
                android:textColor="#3A3939"
                android:textSize="16sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginTop="10dp"
                android:text="Phone No.- 7839358269 (Click to call)"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/m"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginLeft="135dp"
                android:layout_marginTop="-25dp"
                android:text="BOOK NOW" />

        </LinearLayout>
    </LinearLayout>
</ScrollView>