                    android:background="@drawable/cateringcopy" />

            </LinearLayout>
        </HorizontalScrollView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="269dp"
            android:layout_weight="1"
            android:orientation="vertical"
            tools:ignore="ExtraText">

